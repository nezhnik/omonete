{
  "name": "omonete-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:lan": "next dev -H 0.0.0.0",
    "build": "npm run data:rectangular && npm run mints:webp && npm run data:export && node scripts/move-api-for-export.js off && next build && node scripts/move-api-for-export.js on",
    "build:vercel": "next build",
    "mints:webp": "node scripts/convert-mints-to-webp.js",
    "data:rectangular": "node scripts/export-rectangular-from-xlsx.js",
    "data:export": "node scripts/export-coins-to-json.js",
    "start": "next start",
    "lint": "eslint",
    "coins:sync": "node scripts/download-and-optimize-coins.js",
    "coins:sync:all": "node scripts/download-and-optimize-coins.js --all",
    "coins:dzi": "node scripts/download-dzi-coins.js",
    "coins:dzi:all": "node scripts/download-dzi-coins.js --all",
    "coins:fill-mint": "node scripts/fetch-cbr-characteristics.js --fill-mint-only --all",
    "coins:fetch-rectangular": "node scripts/fetch-cbr-characteristics.js --rectangular-only",
    "coins:check-mint": "node scripts/check-mint-issues.js",
    "coins:clean-mint": "node scripts/clean-mint-in-db.js",
    "coins:split-mint": "node scripts/split-mint-to-full-and-short.js",
    "backup:coins": "node scripts/backup-coins-data.js",
    "mints:export": "curl -s http://localhost:3000/api/mint-articles-export -o mint-articles.json",
    "mints:export:standalone": "npx tsx scripts/export-mint-articles-standalone.ts",
    "mints:import": "node scripts/import-mint-articles-to-db.js",
    "metal-prices:cron": "node scripts/cron-metal-prices.js",
    "build:regru": "node scripts/backfill-cbr-rates.js && node scripts/backfill-copper-ruscable.js && npm run metal-prices:cron && npm run build",
    "backfill:all": "node scripts/backfill-cbr-rates.js && FORCE_METAL_CRON=1 node scripts/cron-metal-prices.js && node scripts/backfill-copper-ruscable.js",
    "favicon": "node scripts/logo-to-favicon.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.97.0",
    "@tabler/icons-react": "^3.36.1",
    "lottie-react": "^2.4.1",
    "next": "16.1.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.3.1",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "mysql2": "^3.17.3",
    "png-to-ico": "^2.1.8",
    "sharp": "^0.34.5",
    "tailwindcss": "^4",
    "typescript": "^5",
    "xlsx": "^0.18.5"
  }
}
